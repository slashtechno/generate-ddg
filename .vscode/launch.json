{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run (no args)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [],
            "console": "integratedTerminal"
        },
        {
            "name": "Config",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["--log-level", "debug", "config"],
            "console": "integratedTerminal",
            
        },
        {
            "name": "Test config",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["--log-level", "debug", "config", "--config", "testing.config.yaml", "--secrets", "testing.secrets.yaml"],
            "console": "integratedTerminal",
            
        },
    ],
